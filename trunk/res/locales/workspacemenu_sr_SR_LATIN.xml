<?xml version="1.0" encoding="UTF-8"?>

<menubar>
	<!-- Those starting with "c_" are Standard Containers, and those starting with "p_" are standard parts -->
 
	<container id="c_popup_root" text="Editovanje" mnemonic="E">
		<part id="p_unredo">
			<action id="undo" />
			<action id="redo" />
		</part>
		<part id="p_copypaste">
			<action id="cut" />
			<action id="copy" />
			<action id="paste" />
		</part>
		<part id="p_properties">
			<action id="properties" />
		</part>
	</container>

</menubar>